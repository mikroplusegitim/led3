# Firebase Configuration
# Firebase Console'dan alınacak: https://console.firebase.google.com
VITE_FIREBASE_API_KEY=your-api-key-here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef
VITE_FIREBASE_MEASUREMENT_ID=G-ABCDEFG

# HiveMQ Cloud Configuration
# HiveMQ Cloud Console'dan alınacak: https://console.hivemq.cloud
VITE_HIVEMQ_BROKER_URL=wss://your-cluster.s1.eu.hivemq.cloud:8884/mqtt
VITE_HIVEMQ_USERNAME=your-username
VITE_HIVEMQ_PASSWORD=your-password
